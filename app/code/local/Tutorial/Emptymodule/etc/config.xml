<?xml version="1.0" encoding="UTF-8"?><!-- ** * * *** --><config>    <modules>        <Tutorial_Emptymodule>            <version>0.1</version>        </Tutorial_Emptymodule>        <!-- Modules declarations: names, statuses, dependencies. -->    </modules>    <global>        <!-- Definitions shared among all scopes. -->        <!-- DB settings and Creds, adapter and conn types, -->        <!-- Core module class names. -->        <blocks>            <Tutorial_Emptymodule>                <class>                    Tutorial_Emptymodule_Blocks                </class>            </Tutorial_Emptymodule>        </blocks>        <models>            <Tutorial_Emptymodule>                <class>                    Tutorial_Emptymodule_Models                </class>            </Tutorial_Emptymodule>        </models>        <helpers>            <Tutorial_Emptymodule>                <class>                    Tutorial_Emptymodule_Helpers                </class>            </Tutorial_Emptymodule>        </helpers>        <unspeakable>            <_global>                <_option>                    <Tutorial_Emptymodule>                        <value>"Global"</value>                    </Tutorial_Emptymodule>                </_option>            </_global>        </unspeakable>    </global>    <default>        <!-- Definitions shared among all stores configurations. -->        <!-- Sys locale and options, Design and Theme configuration -->        <!-- Dir structure real folder names. -->        <some>            <random>                <xpath>Here is a value.</xpath>            </random>        </some>        <unspeakable>            <_default>                <_option>                    <Tutorial_Emptymodule>                        <value>"Default"</value>                    </Tutorial_Emptymodule>                </_option>            </_default>        </unspeakable>    </default>    <stores>        <!-- Below are StoreID nodes, overriding <default> info per-store -->        <french>            <some>                <random>                    <xpath>Here is a FRENCH value.</xpath>                </random>            </some>            <unspeakable>                <_default>                    <_option>                        <Tutorial_Emptymodule>                            <value>"FRENCH"</value>                        </Tutorial_Emptymodule>                    </_option>                </_default>            </unspeakable>        </french>    </stores>    <frontend>        <!-- Definitions that require only for frontend area. -->        <!-- Routers, Translation files, Layout XML files, Observers declarations. -->        <unspeakable>            <_frontend>                <_option>                    <Tutorial_Emptymodule>                        <value>"Frontend"</value>                    </Tutorial_Emptymodule>                </_option>            </_frontend>        </unspeakable>    </frontend></config>